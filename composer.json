{
    "name": "glenntenorio/laravel-ilovepdf",
    "type": "library",
    "description": "iLovePDF PHP API for Laravel",
    "homepage": "https://github.com/glenntenorio/laravel-ilovepdf",
    "license": "MIT",
    "authors": [
        {
            "name": "Glenn Ace Tenorio",
            "email": "gatenorio@live.com",
            "homepage": "http://www.glennacetenorio.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "GlennTenorio\\LaravelIlovepdf\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GlennTenorio\\LaravelIlovepdf\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php" : ">=7.0",
        "ext-curl": "*",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "GlennTenorio\\LaravelIlovepdf\\LaravelIlovepdfServiceProvider"
            ],
            "aliases": {
                "IlovePDF": "GlennTenorio\\LaravelIlovepdf\\Facades\\Ilovepdf"
            }
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    }
}
